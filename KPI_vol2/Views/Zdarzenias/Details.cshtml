@model ZdarzenieDetailsVM

@{
    ViewData["Title"] = "Szcegóły zdarzenia";
}



<div>
    <h4>Szcegóły zdarzenia</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Tytuł:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Zdarzenia.Name)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Zdarzenia.Opis):
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Zdarzenia.Opis)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Zdarzenia.Naprawa):
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Zdarzenia.Naprawa)
        </dd>
        <dt class = "col-sm-2">
            Data zdarzenia
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Zdarzenia.DataZdarzenia)
        </dd>
        <dt class = "col-sm-2">
            Data wykonania
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Zdarzenia.DataWykonania)
        </dd>
        <dt class = "col-sm-2">
            Osoba odpowiedzialna
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Zdarzenia.OsobaOdpowiedzialna)
        </dd>
        <dt class="col-sm-2">
            Status zgłoszenia
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Zdarzenia.Status.NameStatus)
          
        </dd>
    </dl>
</div>
<div>
   @* <a asp-action="Edit" asp-route-id="@Model?.Id">Edit</a> |*@
    <a asp-action="Index">Powrót do listy zdarzeń</a>
</div>
